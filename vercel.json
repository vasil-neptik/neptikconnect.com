{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.neptikconnect.com"
        }
      ],
      "destination": "https://neptik.com/:path*",
      "permanent": true
    }
  ]
}